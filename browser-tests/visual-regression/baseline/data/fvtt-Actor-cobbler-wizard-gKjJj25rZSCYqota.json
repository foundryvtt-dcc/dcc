    {
      "name": "Eldritch Hound",
      "type": "spell",
      "flags": {},
      "img": "modules/dcc-core-book/assets/svg/game-icons-net/hound.svg",
      "effects": [],
      "_id": "D5gk41YYeo7scIBF",
      "_stats": {
        "compendiumSource": null,
        "duplicateSource": null,
        "coreVersion": "12.343",
        "systemId": "dcc",
        "systemVersion": "0.51.61",
