    {
      "_id": "dVTfsNEmWXLP7PKt",
      "name": "Patron Bond",
      "type": "spell",
      "flags": {},
      "img": "modules/dcc-core-book/assets/svg/game-icons-net/linked-rings.svg",
      "effects": [],
      "_stats": {
        "compendiumSource": null,
        "duplicateSource": null,
        "coreVersion": "12.343",
        "systemId": "dcc",
        "systemVersion": "0.51.61",
        "createdTime": 1749930766089,
